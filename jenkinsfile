pipeline {
    agent any
    stages{
        stage("GitClone"){
            steps{
              git 'git@github.com:ashanti88/webserver_with_jenkins.git'
            }

        }
    
        stage("Copy file wo Web"){
            steps{
                sh "cp -f index.html /var/www/html/index.html"
            }
        }
    }
}